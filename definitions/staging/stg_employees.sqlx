config {
    type: "view",
    schema: "staging", 
    tags: ["staging", "staff"], 
    assertions: {
        nonNull: ["sales_repr_id", "location_id"], 
        uniqueKey: ["sales_repr_id"]
    }
}

select 
    *
    , concat(first_name, " ", last_name) as full_name
from ${ref("employees")}
